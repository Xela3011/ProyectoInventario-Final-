
@model ProyectoInventario.Models.asientoModel
@{
    /**/

    /**/

    ViewBag.Title = "Crear Asiento";
}

<head>
    <link href="~/Content/main.css" rel="stylesheet" />
</head>
<h2>Enviar Asiento</h2>

@if (Model != null)
{
    <p class="has-error">No se encontraron transacciones en este rango de fecha</p>
}




@using (Html.BeginForm("Create", "asientos", FormMethod.Post))
{

    <div class="form-group">
        @Html.LabelFor(model => model.descripcionAsiento, htmlAttributes: new { @class = "control-label col-md-2 margin-bottom" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.descripcionAsiento, new { htmlAttributes = new { @class = "form-control margin-bottom" } })
            @Html.ValidationMessageFor(model => model.descripcionAsiento, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.fechaDesde, htmlAttributes: new { @class = "control-label col-md-2 margin-bottom" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.fechaDesde, new { htmlAttributes = new { @class = "form-control datePicker margin-bottom" } })
            @Html.ValidationMessageFor(model => model.fechaDesde, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.fechaHasta, htmlAttributes: new { @class = "control-label col-md-2 margin-bottom" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.fechaHasta, new { htmlAttributes = new { @class = "form-control datePicker margin-bottom" } })
            @Html.ValidationMessageFor(model => model.fechaHasta, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Enviar" class="btn btn-default bg-success" />
        </div>
    </div>

}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/moment.js"></script>">
    <script src="~/Scripts/bootstrap-datetimepicker.js"></script>
    @Scripts.Render("~/Scripts/main.js")


}